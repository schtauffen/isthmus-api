{
  "ignore": "node_modules",
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6.10.3"
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": false
    }],
    "ramda"
  ]
}
